{
    "name": "cartalyst/permissions",
    "description": "A framework agnostic permissions storage container.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "permissions"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cartalyst/support": "~1.1|~2.0"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\Permissions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\Permissions\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
